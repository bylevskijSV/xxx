<% content_for :head do %>
  <%= stylesheet_link_tag 'styles_delivery', media: 'all', 'data-turbolinks-track': 'reload' %>
<% end %>

<section>
  <div class="cart">
    <div class="cart-headline">
          <p>Корзина</p>
    </div>

    <div class="cart-content">
        <div class="main-cart">
            <%= render 'cart_item' %>
        </div>
    </div>

    <div class="cart-footer">
        <button><%= link_to "Перейти в меню", home_index_path %></button>
        <button><%= link_to "Оформить закать" %></button>
    </div>
  </div>
</section>